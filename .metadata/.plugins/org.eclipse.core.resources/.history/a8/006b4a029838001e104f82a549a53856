package Solid_aula2;

public class cartao_de_credito {
	
	public String nome;
	public String numero;
	public String data_validade;
	
	
	public boolean ValidadaNome() {
		private boolean resultado=false;
		//chamar o endpoint da api da operadora de cart√£o de credito 
		if this.nome.equalsIgnoreCase("Pedro de lara"){
			resultado = true;
				
		}else {
			resultado = false;
		}
		return resultado;
	}
	
	

}
